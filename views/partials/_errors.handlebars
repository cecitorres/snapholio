{{#if error}}
  <div class="alert alert-danger">
    <p style="width: 15px;"><img src="img/icon-144x144.png" style="width: 30px;"> {{error}}</p>
  </div>
{{/if}}

{{#each errors}}
  <div class="alert alert-danger">
    <p style="height: 15px;"><img src="img/icon-144x144.png" style="width: 30px;"> {{text}}</p>
  </div>
{{else}}

{{/each}}